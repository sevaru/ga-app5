{
	"presets": [
    	"es2015",
    	"react",
    	"stage-0"
	],
	"plugins": [
		["typecheck", {
	      "disable": {
	        "production": true
	      }
	    }],
		"transform-flow-strip-types",
		"syntax-flow",
	
		"syntax-class-properties",
		"syntax-decorators",
		"syntax-object-rest-spread",

		"transform-class-properties",
		"transform-decorators-legacy",
		"transform-object-rest-spread"
	],
	"env": {
		"start": {
			"presets": [
				"react-hmre"
			]
		}
	}
}